[user]
    name = Hyunjin CHA
    email = gump.cha@gmail.com
[core]
    excludefiles = ~/.gitignore
[color]
    ui = auto
[alias]
    ci = commit
    co = checkout
    st = status
    ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --graph
    ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
    ld = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --graph --date=short
    br = branch -v
